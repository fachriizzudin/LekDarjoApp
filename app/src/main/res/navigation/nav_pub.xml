<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/nav_pub"
    app:startDestination="@id/publicationListFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.GraphFragment"
        android:label="MainFragment" >
        <action
            android:id="@+id/actionMainToPublicationList"
            app:destination="@id/publicationListFragment" />
        <action
            android:id="@+id/actionMainToIndicatorList"
            app:destination="@id/indicatorListFragment" />
        <action
            android:id="@+id/actionMainInfographicList"
            app:destination="@id/infographicListFragment" />
        <action
            android:id="@+id/actionMainToAbout"
            app:destination="@id/aboutFragment" />
        <action
            android:id="@+id/action_mainFragment_to_statisticalNewsListFragment"
            app:destination="@id/statisticalNewsListFragment" />
        <action
            android:id="@+id/actionMainToEconomy"
            app:destination="@id/economyGrowthFragment2" />
        <action
            android:id="@+id/actionMainToIpm"
            app:destination="@id/ipmFragment" />
        <action
            android:id="@+id/actionMainToPopulation"
            app:destination="@id/populationFragment" />
        <action
            android:id="@+id/actionMainToPoverty"
            app:destination="@id/povertyFragment" />
        <action
            android:id="@+id/actionMainToUnemployment"
            app:destination="@id/unemploymentFragment" />
        <action
            android:id="@+id/actionMainToRiceProduction"
            app:destination="@id/riceProductionFragment" />
        <action
            android:id="@+id/actionMainToRiceProductivity"
            app:destination="@id/riceProductivityFragment" />
    </fragment>
    <fragment
        android:id="@+id/publicationListFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.list_fragment.PublicationListFragment"
        android:label="PublicationListFragment" >
        <action
            android:id="@+id/publicationActionListToDetail"
            app:destination="@id/publicationDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/publicationDetailFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.detail_fragment.PublicationDetailFragment"
        android:label="PublicationDetailFragment" >
        <action
            android:id="@+id/actionDetailToList"
            app:destination="@id/publicationListFragment" />
        <argument
            android:name="uuid"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/indicatorListFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.list_fragment.IndicatorPagerFragment"
        android:label="IndicatorListFragment" />
    <fragment
        android:id="@+id/infographicListFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.list_fragment.InfographicListFragment"
        android:label="InfographicListFragment" >
        <action
            android:id="@+id/infographicActionListToDetail"
            app:destination="@id/infographicDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.AboutFragment"
        android:label="AboutFragment" />
    <fragment
        android:id="@+id/statisticalNewsListFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.list_fragment.StatisticalNewsListFragment"
        android:label="StatisticalNewsListFragment" >
        <action
            android:id="@+id/actionStatisticalNewsListToDetail"
            app:destination="@id/statisticalNewsDetailFragment" />
    </fragment>
    <fragment
        android:id="@+id/statisticalNewsDetailFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.detail_fragment.StatisticalNewsDetailFragment"
        android:label="StatisticalNewsDetailFragment" >
        <action
            android:id="@+id/actionStatisticalNewsToList"
            app:destination="@id/statisticalNewsListFragment" />
        <argument
            android:name="uuid"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/infographicDetailFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.detail_fragment.InfographicDetailFragment"
        android:label="InfographicDetailFragment" >
        <argument
            android:name="uuid"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/economyGrowthFragment2"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.graph_fragment.EconomyGrowthFragment"
        android:label="EconomyGrowthFragment" >
        <argument
            android:name="id"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/ipmFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.graph_fragment.IpmFragment"
        android:label="IpmFragment" >
        <argument
            android:name="id"
            app:argType="integer"
            android:defaultValue="0" />
    </fragment>
    <fragment
        android:id="@+id/populationFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.graph_fragment.PopulationFragment"
        android:label="PopulationFragment" />
    <fragment
        android:id="@+id/povertyFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.graph_fragment.PovertyFragment"
        android:label="PovertyFragment" />
    <fragment
        android:id="@+id/unemploymentFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.graph_fragment.UnemploymentFragment"
        android:label="UnemploymentFragment" />
    <fragment
        android:id="@+id/riceProductionFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.graph_fragment.RiceProductionFragment"
        android:label="RiceProductionFragment" />
    <fragment
        android:id="@+id/riceProductivityFragment"
        android:name="com.lazuardifachri.bps.lekdarjoapp.view.graph_fragment.RiceProductivityFragment"
        android:label="RiceProductivityFragment" />
</navigation>